        "Runtime": "nodejs6.10",
        "Timeout": 300,
        "Environment": {
          "Variables": {
            "LOG_LEVEL": "debug",
            "LISTS_TABLE": "MoonMail-v2-dev-lists",
            "RECIPIENTS_TABLE": "MoonMail-v2-dev-recipients",
            "ES_HOST": "https://search-es-microapps-odcj3qud4olgytj6sz4bfy3nxm.us-east-1.es.amazonaws.com/",
            "ES_REGION": "us-east-1"
          }
        }
      },
      "DependsOn": [
        "ImportRecipientsLogGroup",
        "IamRoleLambdaExecution"
      ]
    },
    "ImportRecipientsLambdaVersionGhG7ksrY5pDdbXludgcbzaqqVUOYAweuC1LRHbqLY": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "ImportRecipientsLambdaFunction"
        },
        "CodeSha256": "GhG7ksrY5pDdbX+ludgcbzaqqVUOYA/weuC1LRHbqLY="
      }
    },
    "RecipientImportedEventHandlerLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/MoonMail-v2-lists/dev/1508943182056-2017-10-25T14:53:02.056Z/recipientImportedEventHandler.zip"
        },
        "FunctionName": "MoonMail-v2-lists-dev-recipientImportedEventHandler",
        "Handler": "_optimize/MoonMail-v2-lists-dev-recipientImportedEventHandler/lib/Recipients.recipientImportedHandler",
        "MemorySize": 1024,
        "Role": {
          "Fn::GetAtt": [
            "IamRoleLambdaExecution",
            "Arn"
          ]
        },
        "Runtime": "nodejs6.10",
        "Timeout": 300,
        "Environment": {
          "Variables": {
            "LOG_LEVEL": "debug",
            "LISTS_TABLE": "MoonMail-v2-dev-lists",
            "RECIPIENTS_TABLE": "MoonMail-v2-dev-recipients",
            "ES_HOST": "https://search-es-microapps-odcj3qud4olgytj6sz4bfy3nxm.us-east-1.es.amazonaws.com/",
            "ES_REGION": "us-east-1"
          }
        }
      },
      "DependsOn": [
        "RecipientImportedEventHandlerLogGroup",
        "IamRoleLambdaExecution"
      ]
    },
    "RecipientImportedEventHandlerLambdaVersionVr18R0fZNZPqYMxKiXUf2aXvnUZQxFayfivGCBMJDBU": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "RecipientImportedEventHandlerLambdaFunction"
        },
        "CodeSha256": "Vr18R0fZNZPqYMxKiXUf2aXvnUZQxFayfivGCBMJDBU="
      }
    },
    "S3BucketNextrecipientsmoonmaildevuseast1": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "next-recipients.moonmail.dev.us-east-1",
        "NotificationConfiguration": {
          "LambdaConfigurations": [
            {
              "Event": "s3:ObjectCreated:*",
              "Function": {
                "Fn::GetAtt": [
                  "ImportRecipientsLambdaFunction",
                  "Arn"
                ]
              }
            }
          ]
        }
      },
      "DependsOn": [
        "ImportRecipientsLambdaPermissionNextrecipientsmoonmaildevuseast1S3"
      ]
    },
    "ImportRecipientsLambdaPermissionNextrecipientsmoonmaildevuseast1S3": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "FunctionName": {
          "Fn::GetAtt": [
            "ImportRecipientsLambdaFunction",
            "Arn"
          ]
        },
        "Action": "lambda:InvokeFunction",
        "Principal": "s3.amazonaws.com",
        "SourceArn": {
          "Fn::Join": [
            "",
            [
              "arn:aws:s3:::next-recipients.moonmail.dev.us-east-1"
            ]
          ]
        }
      }
    },
    "RecipientImportedEventHandlerEventSourceMappingKinesisListEventStream": {
      "Type": "AWS::Lambda::EventSourceMapping",
      "DependsOn": "IamRoleLambdaExecution",
      "Properties": {
        "BatchSize": 100,
        "EventSourceArn": {
          "Fn::GetAtt": [
            "ListEventStream",
            "Arn"
          ]
        },
        "FunctionName": {
          "Fn::GetAtt": [
            "RecipientImportedEventHandlerLambdaFunction",
            "Arn"
          ]
        },
        "StartingPosition": "TRIM_HORIZON",
        "Enabled": "True"
      }
    },
    "ListEventStream": {
      "Type": "AWS::Kinesis::Stream",
      "Properties": {
        "Name": "ListEventStream",
        "ShardCount": 1
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      }
    },
    "ImportRecipientsLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "ImportRecipientsLambdaVersionGhG7ksrY5pDdbXludgcbzaqqVUOYAweuC1LRHbqLY"
      }
    },
    "RecipientImportedEventHandlerLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "RecipientImportedEventHandlerLambdaVersionVr18R0fZNZPqYMxKiXUf2aXvnUZQxFayfivGCBMJDBU"
      }
    },
    "ListEventStreamARN": {
      "Description": "ListEventStream Kinesis Stream ARN",
      "Value": {
        "Fn::GetAtt": [
